#!groovy
//check mine_server properties
properties([disableConcurrentBuilds()])

pipeline {
    agent {
	    label 'master'
	    }
	options {
		buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
		timestamps()
	}
    stages {
		stage("first step") {
		    steps {
			    sh 'ssh root@mine_server \'hostname\''
		    }
		stage("second step") {
					sh ssh root@mine_server \'uptime\''
}
}
}
}